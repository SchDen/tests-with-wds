{
  "name": "webpack-mocha-tests",
  "version": "0.1.0",
  "description": "Starter kit for testing JavaScript app with webpack, mocha, and chai",
  "main": "index.js",
  "scripts": {
    "test": "webpack --config webpack.tests.config.js --inline -c --progress && mocha ./tests/test.build.js && rm ./tests/test.build.js",
    "test-dev": "NODE_ENV=develop webpack-dev-server --config webpack.tests.config.js --inline"
  },
  "repository": "https://github.com/SchDen/tests-with-wds",
  "author": "Den",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "chai": "^3.5.0",
    "mocha": "^2.3.3",
    "mocha-loader": "^0.7.1",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  }
}
